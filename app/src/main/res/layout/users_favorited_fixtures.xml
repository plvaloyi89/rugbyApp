<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="160dp"
    app:cardCornerRadius="10dp"
    app:cardElevation="1dp"
    app:cardMaxElevation="1dp"
    android:padding="0dp"
    android:layout_marginTop="5dp"
    android:layout_marginBottom="5dp"
    android:layout_marginRight="5dp"
    android:layout_marginLeft="5dp">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/structure"
        android:layout_width="match_parent"
        android:layout_height="160dp"
        android:layout_marginTop="0dp">


        <ImageView
            android:id="@+id/savedHomeTeamImage"
            android:layout_width="58dp"
            android:layout_height="75dp"
            android:layout_marginStart="30dp"
            android:layout_marginLeft="30dp"
            app:layout_constraintBottom_toBottomOf="@id/structure"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.013"
            app:layout_constraintStart_toStartOf="@id/structure"
            app:layout_constraintTop_toTopOf="@id/structure"
            app:layout_constraintVertical_bias="0.093"
            tools:srcCompat="@tools:sample/avatars" />

        <ImageView
            android:id="@+id/savedAwayTeamImage"
            android:layout_width="58dp"
            android:layout_height="75dp"
            android:layout_marginEnd="32dp"
            android:layout_marginRight="32dp"
            app:layout_constraintBottom_toBottomOf="@id/structure"
            app:layout_constraintEnd_toEndOf="@id/structure"
            app:layout_constraintHorizontal_bias="0.928"
            app:layout_constraintStart_toEndOf="@+id/savedHomeTeamImage"
            app:layout_constraintTop_toTopOf="@id/structure"
            app:layout_constraintVertical_bias="0.102"
            tools:srcCompat="@tools:sample/avatars" />

        <TextView
            android:id="@+id/savedawayTeam"
            android:layout_width="104dp"
            android:layout_height="36dp"
            android:layout_marginTop="5dp"
            android:layout_marginEnd="16dp"
            android:layout_marginRight="16dp"
            android:gravity="center"
            android:text="TextView"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/savedAwayTeamImage"
            app:layout_constraintVertical_bias="0.0"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/versus"
            android:layout_width="53dp"
            android:layout_height="26dp"
            android:gravity="center"
            android:text="VS"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/savedAwayTeamImage"
            app:layout_constraintHorizontal_bias="0.545"
            app:layout_constraintStart_toEndOf="@+id/savedHomeTeamImage"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/savedhomeTeam"
            android:layout_width="96dp"
            android:layout_height="36dp"
            android:layout_marginStart="16dp"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="5dp"
            android:gravity="center"
            android:text="TextView"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/savedHomeTeamImage"
            app:layout_constraintVertical_bias="0.027"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/savedfixturedate"
            android:layout_width="107dp"
            android:layout_height="18dp"
            android:gravity="center"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/versus"
            app:layout_constraintEnd_toStartOf="@+id/savedAwayTeamImage"
            app:layout_constraintHorizontal_bias="0.576"
            app:layout_constraintStart_toEndOf="@+id/savedHomeTeamImage"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.074" />

        <TextView
            android:id="@+id/savedtime"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="5dp"
            android:gravity="center"
            android:textStyle="bold"
            android:text="Time"
            app:layout_constraintBottom_toTopOf="@+id/versus"
            app:layout_constraintEnd_toStartOf="@+id/savedAwayTeamImage"
            app:layout_constraintHorizontal_bias="0.535"
            app:layout_constraintStart_toEndOf="@+id/savedHomeTeamImage"
            app:layout_constraintTop_toBottomOf="@+id/savedfixturedate"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/savedcompName"
            android:layout_width="77dp"
            android:layout_height="36dp"
            android:gravity="center"
            android:text="TextView"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/savedawayTeam"
            app:layout_constraintHorizontal_bias="0.543"
            app:layout_constraintStart_toEndOf="@+id/savedhomeTeam"
            app:layout_constraintTop_toBottomOf="@+id/versus"
            app:layout_constraintVertical_bias="0.428" />

        <ImageView
            android:id="@+id/delete_fixture"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:labelFor="@id/delete_fixture"
            android:layout_marginEnd="4dp"
            android:layout_marginRight="4dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0"
            app:srcCompat="@android:drawable/ic_delete"
            tools:ignore="VectorDrawableCompat" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.cardview.widget.CardView>
